<?xml version="1.0" encoding="UTF-8"?>
<returnEntity>
    <className>Message</className>
    <queries>
        <name>findSample</name>
        <author>saiki</author>
        <desc>
            これはサンプルです
        </desc>
        <statement>
            select * from
            tab where a =
            :param1 b = :param2
        </statement>
        <parameters>
            <name>param1</name>
            <type>String</type>
        </parameters>
        <parameters>
            <name>param2</name>
            <type>UserAccount</type>
        </parameters>
    </queries>
    <queries>
        <name>findNotAccessMessage</name>
        <author>saiki</author>
        <desc>
            これもサンプルです
        </desc>
        <statement>
            select * from
            tab where a =
            param1 = :paramX
            and
            param2 = :paramY
            or
            param3 in :paramZ
        </statement>
        <parameters>
            <name>paramX</name>
            <type>String</type>
        </parameters>
        <parameters>
            <name>paramY</name>
            <type>UserAccount</type>
        </parameters>
        <parameters>
            <name>paramZ</name>
            <type>List&lt;XXEntity&gt;</type>
        </parameters>
    </queries>
</returnEntity>